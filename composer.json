{
    "name": "LEARNING-ANALYTICS-WEB-APPLICATION",
    "license": "ECL",
    "type": "project",
    "description": "Learning Analytics Web Application. Created by University of Lorraine",
    "authors": [
        {
            "name": "Xavier Chopin",
            "email": "xavier.chopin@univ-lorraine.fr",
            "homepage": "https://github.com/xchopin"
        }
    ],
    "require": {
        "php":                           "^7.1.3",
        "openlrw/api-client":            "^0.2.1",
        "sensio/framework-extra-bundle": "^5.2.4",
        "symfony/flex":                  "^1.2.1",
        "symfony/profiler-pack":         "^1.0.4",
        "doctrine/annotations":          "^1.6.0",
        "symfony/monolog-bundle":        "^3.3.1",
        "symfony/translation":           "^4.2.5",
        "symfony/asset":                 "^4.2.5",
        "symfony/console":               "^4.2.5",
        "symfony/security-core":         "^4.2.5",
        "symfony/security-csrf":         "^4.2.5",
        "symfony/twig-bundle":           "^4.2.5",
        "symfony/validator":             "^4.2.5",
        "symfony/yaml":                  "^4.2.5",
        "jms/i18n-routing-bundle":       "^3.0.2",
        "jasig/phpcas":                  "^1.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "^4.2.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [

        ],
        "post-update-cmd": [

        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C08SGJJ4QF312G82T7GEBTKW",
            "allow-contrib": false
        }
    }
}
