{
    "name": "xchopin/mad",
    "license": "ECL",
    "type": "project",
    "description": "A scalable Web Dashboard for a Learning Analytics usage. Created by University of Lorraine",
    "authors": [
        {
            "name": "Xavier Chopin",
            "email": "xavier.chopin@univ-lorraine.fr",
            "homepage": "https://github.com/xchopin"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/annotations": "1.8.0",
        "jasig/phpcas": "^1.3.7",
        "jms/i18n-routing-bundle": "^3.0.3",
        "openlrw/api-client": "0.2.9",
        "ozdemirburak/iris": "1.2.1",
        "sensio/framework-extra-bundle": "^5.2.4",
        "symfony/asset": "4.4.1",
        "symfony/console": "4.4.1",
        "symfony/flex": "1.6.0",
        "symfony/monolog-bundle": "3.5.0",
        "symfony/profiler-pack": "1.0.4",
        "symfony/security-bundle": "4.4.1",
        "symfony/stopwatch": "4.4.1",
        "symfony/translation": "4.4.1",
        "symfony/twig-bundle": "4.4.1",
        "symfony/validator": "4.4.1",
        "symfony/yaml": "4.4.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "4.4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C08SGJJ4QF312G82T7GEBTKW",
            "allow-contrib": false
        }
    }
}
